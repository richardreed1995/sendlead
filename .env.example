# Slack Webhook URL for bootcamp form notifications
# Get this from your Slack workspace: Apps > Incoming Webhooks
NEXT_PUBLIC_SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL

# EmailJS Configuration for email notifications
NEXT_PUBLIC_EMAILJS_SERVICE_ID=your_service_id_here

# Kit (formerly ConvertKit) Configuration
KIT_API_KEY=your_kit_api_key_here
KIT_SEQUENCE_ID=2487293
